{
  "name": "geocell",
  "version": "1.2.0",
  "description": "Geographical position conversion library for JavaScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/pjarts/geocell"
  },
  "main": "./dist/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register",
